package com.condenast{	import flash.display.Sprite;	import com.greensock.TimelineLite;	import com.greensock.TweenMax;	import com.greensock.easing.Quint;	import flash.events.MouseEvent;		public class Product extends Sprite {		private var _lib:Object;		private var _track:Function;				public function Product($lib:Object, $f:Function) {			_lib = $lib;			_track = $f;			init();		}		private function init():void{			//Your code goes here!			var count:uint;			var loop:uint;			var slides:Sprite = new Sprite;			var hit:Sprite = new Sprite;			var image_lists:Array = new Array;			hit.graphics.beginFill(0x00d9f9, 0);			hit.graphics.drawRect(0, 0, 970, 90);			hit.graphics.endFill();			slides.x = 370;			for(var el:String in _lib){				if(el.split('image').length > 1){					_lib[el].x = 600*(uint(el.split('image')[1]));					slides.addChild(_lib[el]);					image_lists[uint(el.split('image')[1])] = _lib[el];				}			}			addChild(slides)			addChild(_lib.header);			addChild(hit);			var t:TimelineLite = new TimelineLite({onComplete:auto});			t.insert(TweenMax.from(_lib.header, .5, {x:-300, ease:Quint.easeOut}));			t.insert(TweenMax.from(slides, .5, {x:970, ease:Quint.easeOut}), .1);						hit.buttonMode = true;			hit.addEventListener(MouseEvent.MOUSE_DOWN, function():void{ _track('click', 'Nav - Expand'); _track('message', 'expand');});			function auto():void{				TweenMax.to(slides, .5, {x:(-(count)*600 + 370), onComplete:function():void{					TweenMax.to({}, 3, {onComplete:function():void{						if(count+1<image_lists.length && loop<2){							count++;						}else{							count = 0;							loop++;						}						auto();					}});				}});			}		}	}	}